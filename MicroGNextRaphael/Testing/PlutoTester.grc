    rotation: 0
    state: disabled
- name: blocks_repeat_0
  id: blocks_repeat
  parameters:
    affinity: ''
    alias: ''
    comment: 'We repeat this by samples_per_bit/2

      to get rectangular BPSK.

      (We divide by 2 since manchester encoded

      BPSK has half the data rate of regular BPSK)'
    interp: '850'
    maxoutbuf: '0'
    minoutbuf: '0'
    type: complex
    vlen: '1'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
