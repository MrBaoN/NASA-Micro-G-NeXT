- name: blocks_vector_source_x_0
  id: blocks_vector_source_x
  parameters:
    affinity: ''
    alias: ''
    comment: "This is our initial symbol source. The \nbit sequence is (1, 0, 0) repeating,\
      \ so the\nmanchester encoded bits are\n(1, -1, -1, 1, -1, 1) repeating."
    maxoutbuf: '0'
    minoutbuf: '0'
    repeat: 'True'
    tags: '[]'
    type: complex
    vector: (1, -1, -1, 1, -1, 1,)
    vlen: '1'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
